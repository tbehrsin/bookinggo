{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "jsx-a11y/no-autofocus": "off",
    "react/no-array-index-key": "off",
    "react/jsx-filename-extension": ["error", { "extensions": [".js"] }]
  }
}
